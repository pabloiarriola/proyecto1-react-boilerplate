{
  "name": "proyecto1-react-boilerplate",
  "version": "1.0.0",
  "description": "Basic React Boilerplate for the proyect 1 of Web Development",
  "main": "./dist/server.js",
  "scripts": {
    "build": "webpack",
    "build-babel": "npm run build-babel-server && npm run build-babel-client",
    "build-babel-server": "babel src/server --out-dir ./dist",
    "build-babel-client": "babel src/client --copy-files --out-dir ./dist/public",
    "clean": "rimraf dist node_modules",
    "start": "node ./dist/server.js",
    "test": "jest ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pabloiarriola/proyecto1-react-boilerplate"
  },
  "keywords": [
    "Node",
    "React"
  ],
  "author": "Pablo Arriola",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/pabloiarriola/proyecto1-react-boilerplate"
  },
  "homepage": "https://github.com/pabloiarriola/proyecto1-react-boilerplate",
  "dependencies": {
    "connected-react-router": "^5.0.1",
    "express": "^4.16.2",
    "history": "^4.7.2",
    "html-webpack-plugin": "^3.2.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "webpack-node-externals": "^1.7.2",
    "react-redux": "^5.1.1",
    "react-router": "4.3.1",
    "react-router-dom": "^4.3.1",
    "react-test-renderer": "^16.2.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "jest": "^21.2.1",
    "react-redux": "^5.1.1",
    "react-router": "4.3.1",
    "react-router-dom": "^4.3.1",
    "react-test-renderer": "^16.2.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "sanitize.css": "7.0.3",
    "webpack": "^3.12.0",
    "webpack-cli": "^2.1.3"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./src/enzyme.setup.js",
    "setupFiles": [
      "raf/polyfill"
    ]
  }
}
